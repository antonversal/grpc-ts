{
  "name": "@join-com/grpcts-health-check",
  "version": "1.0.0-4",
  "description": "health check server and client for grpcts",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "tsc:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "proto:generate": "protoc ./proto/*.proto -I ./proto --ts_out=/$PWD/src/generated",
    "proto:pretty": "yarn prettier --write './src/generated/**/*.ts'",
    "proto:build": "yarn proto:generate && yarn proto:pretty",
    "prepublish": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/join-com/grpc-ts.git",
  "author": "JOIN Team",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://github.com/join-com/grpc-ts",
  "contributors": [
    "Anton Versal <anton.versal@gmail.com>"
  ],
  "keywords": [
    "grpc",
    "protobuf",
    "typescript",
    "health check"
  ],
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.3",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-watch-typeahead": "^0.2.0",
    "prettier": "^1.15.1",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@join-com/grpc-ts": "^1.0.0-4",
    "@join-com/node-trace": "^0.1.5",
    "grpc": "^1.16.0",
    "protobufjs": "^6.8.8"
  }
}
