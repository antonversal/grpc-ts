{
  "name": "@join-com/grpc-ts-mock",
  "version": "0.1.11",
  "description": "grpc mock services",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "tsc:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "prepublish": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/join-com/grpc-ts.git",
  "author": "JOIN Team",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://github.com/join-com/grpc-ts",
  "contributors": [
    "Anton Versal <anton.versal@gmail.com>"
  ],
  "keywords": [
    "grpc",
    "protobuf",
    "typescript",
    "mock",
    "test",
    "jest"
  ],
  "files": [
    "lib/"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testPathIgnorePatterns": [
      "/proto/",
      "/generated/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.1.6",
    "grpc": "^1.13.0",
    "jest": "^23.4.1",
    "jest-cli": "^23.1.0",
    "jest-watch-typeahead": "^0.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@join-com/grpc-ts": "^0.1.11",
    "jest-mock": "^23.2.0"
  }
}
